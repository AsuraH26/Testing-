#include "common.h"

float4 main ( p_TL I ) : SV_Target
{
	float4 color = s_base.Sample( smp_base, I.Tex0 );
	color.rgb = lerp( color.rgb, I.Color.rgb, I.Color.a);
    color.rgb = color.rgb * (color.rgb * (color.rgb * 0.305306011 + 0.682171111) + 0.012522878);
	color.a *= I.Color.a;
	return color;
}
